  <section xml:id="dom-attr.props">
   &reftitle.properties;
   <variablelist>
    <varlistentry xml:id="dom-attr.props.namespaceuri">
     <term><varname>namespaceURI</varname></term>
     <listitem>
      <simpara>属性の名前空間 <acronym>URI</acronym></simpara>
     </listitem>
    </varlistentry>
    <varlistentry xml:id="dom-attr.props.prefix">
     <term><varname>prefix</varname></term>
     <listitem>
      <simpara>属性の名前空間プレフィックス</simpara>
     </listitem>
    </varlistentry>
    <varlistentry xml:id="dom-attr.props.localname">
     <term><varname>localName</varname></term>
     <listitem>
      <simpara>属性のローカル名</simpara>
     </listitem>
    </varlistentry>
    <varlistentry xml:id="dom-attr.props.name">
     <term><varname>name</varname></term>
     <listitem>
      <simpara>属性の修飾名</simpara>
     </listitem>
    </varlistentry>
    <varlistentry xml:id="dom-attr.props.value">
     <term><varname>value</varname></term>
     <listitem>
      <simpara>属性の値</simpara>
      <note>
       <simpara>
        <classname>DOMAttr</classname> の同名のプロパティと異なり、
        この値はエンティティを置換しません。
       </simpara>
      </note>
     </listitem>
    </varlistentry>
    <varlistentry xml:id="dom-attr.props.ownerelement">
     <term><varname>ownerElement</varname></term>
     <listitem>
      <simpara>属性を含む要素。または &null;</simpara>
     </listitem>
    </varlistentry>
    <varlistentry xml:id="dom-attr.props.specified">
     <term><varname>specified</varname></term>
     <listitem>
      <simpara>レガシーオプション。常に &true;</simpara>
     </listitem>
    </varlistentry>
   </variablelist>
  </section>

